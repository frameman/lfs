#!/bin/bash
cd $LFS/sources/man-db-2.11.1

./configure --prefix=/usr                         \
            --docdir=/usr/share/doc/man-db-2.11.1 \
            --sysconfdir=/etc                     \
            --disable-setuid                      \
            --enable-cache-owner=bin              \
            --with-browser=/usr/bin/lynx          \
            --with-vgrind=/usr/bin/vgrind         \
            --with-grap=/usr/bin/grap

make -j3
#make check
make install

if [ accessdb ] 
then
echo "man-db is installed" >> ../make_all/install.log
else
echo "man-db not install" >> ../make_all/install.log
fi
