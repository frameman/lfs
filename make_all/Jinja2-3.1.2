#!/bin/bash
cd $LFS/sources/Jinja2-3.1.2 

pip3 wheel -w dist --no-build-isolation --no-deps $PWD
pip3 install --no-index --no-user --find-links dist Jinja2

if [ /usr/lib/python3.11/site-packages/Jinja2-3.1.2.dist-info ]
then
echo "jinja2 is installed " >> ../make_all/install.log
else
echo "jinja2 not install " >> ../make_all/install.log
fi
