#!/bin/bash
cd $LFS/sources/procps-ng-4.0.1  

./configure --prefix=/usr                           \
            --docdir=/usr/share/doc/procps-ng-4.0.1 \
            --disable-static                        \
            --disable-kill                          \
            --with-systemd

make -j3
#make check
make install

if [ free ]
then
echo "procps is installed" >> ../make_all/install.log
else
echo "procps not install" >> ../make_all/install.log
fi

